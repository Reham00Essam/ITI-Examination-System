  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@instructor_id">
            <Value>=Parameters!instructor_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select IC.Course_Id , COUNT(c.Student_ID) as number_of_students
	from Instructor_Course as IC
	 join Student_Course as c
	on c.Course_Id = IC.Course_Id
	where IC.Instructor_Id in (@instructor_id)
	group by IC.Course_Id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Course_Id">
          <DataField>Course_Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
