  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@instructor_id">
            <Value>=Parameters!instructor_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select IC.Instructor_Id ,c.Course_Name, COUNT(s.Student_ID) as number_of_students
	from Instructor_Course as IC
	 join Student_Course as s
	on s.Course_Id = IC.Course_Id 
	 join Course c
	 on c.Course_Id =s.Course_Id
	where IC.Instructor_Id =@instructor_id
	group by IC.Instructor_Id,c.Course_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Instructor_Id">
          <DataField>Instructor_Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Course_Name">
          <DataField>Course_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="number_of_students">
          <DataField>number_of_students</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
