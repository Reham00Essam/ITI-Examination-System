    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@exam_ID">
            <Value>=Parameters!exam_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select q.Question_Header ,q.Question_Type,e.Exam_Id
	from Exam_Question as e
	join Question as q 
	on e.Question_Id = q.Question_Id
	where e.Exam_Id in (@exam_ID)</CommandText>
      </Query>
