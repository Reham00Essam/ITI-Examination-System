    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@course_ID">
            <Value>=Parameters!course_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.Course_Id,c.Course_Name,t.Topic_Name
	from course c inner join Topic t
	on c.Course_Id = t.Course_Id
	where c.Course_Id=@course_ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
