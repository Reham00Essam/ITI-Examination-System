    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Exam_num">
            <Value>=Parameters!Exam_num.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Stu_id">
            <Value>=Parameters!Stu_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select Question.Question_Header,answer.Student_Answer
from Question,answer
where Question.Question_Id=answer.Question_Id
and answer.Exam_Id=@Exam_num 
and answer.Student_Id=@Stu_id</CommandText>
